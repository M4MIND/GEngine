{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nox-engine-editor-store/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nox-engine-editor-store",
        "main": "libs/nox-engine-editor-store/src/index.ts",
        "tsConfig": "libs/nox-engine-editor-store/tsconfig.lib.json",
        "assets": ["libs/nox-engine-editor-store/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/nox-engine-editor-store/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/nox-engine-editor-store"],
      "options": {
        "jestConfig": "libs/nox-engine-editor-store/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
